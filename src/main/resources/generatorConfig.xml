<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator
Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="testTables" targetRuntime="MyBatis3">
        <commentGenerator>
            <property name="suppressAllComments" value="true" />
        </commentGenerator>
        <jdbcConnection
                driverClass="com.mysql.cj.jdbc.Driver"
                connectionURL="jdbc:mysql://localhost:3306/shopping?serverTimezone=GMT"
                userId="shopping"
                password="shopping">
        </jdbcConnection>
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>
        <!-- targetProject: where to generate po -->
        <javaModelGenerator targetPackage="com.leih.shopping.db.po"
                            targetProject="./src/main/java">
           <property name="enableSubPackages" value="true" />
            <!-- trim strings -->
            <property name="trimStrings" value="true" />
        </javaModelGenerator>
        <!-- targetProject:mapper file generated -->
        <sqlMapGenerator targetPackage="mappers"
                         targetProject="./src/main/resources">
            <!-- enableSubPackages:xxx.schema -->
            <property name="enableSubPackages" value="true" />
        </sqlMapGenerator>
        <!-- targetPackage:where to generate mapper interface -->
        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="com.leih.shopping.db.mappers" targetProject="./src/main/java">
            <!-- enableSubPackages:xxx.schema -->
            <property name="enableSubPackages" value="true" />
        </javaClientGenerator>
        <table schema="" tableName="order"
               domainObjectName="Order" enableCountByExample="false"
               enableDeleteByExample="false" enableSelectByExample="false"
               enableUpdateByExample="false" selectByExampleQueryId="false"></table>
        <table schema="" tableName="user"
               domainObjectName="User" enableCountByExample="false"
               enableDeleteByExample="false" enableSelectByExample="false"
               enableUpdateByExample="false" selectByExampleQueryId="false"></table>
    </context>
</generatorConfiguration>